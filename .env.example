# MongoDB Atlas Configuration
# Get your connection string from MongoDB Atlas console
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/database?retryWrites=true&w=majority

# Voyage AI Configuration  
# Get your API key from https://voyageai.com
VOYAGE_API_KEY=your-voyage-ai-api-key-here

# Optional Configuration
# Database name (default: memory_engineering)
MEMORY_ENGINEERING_DB=memory_engineering

# Collection name (default: memory_engineering_documents)
MEMORY_ENGINEERING_COLLECTION=memory_engineering_documents

# Environment mode (development, production)
NODE_ENV=development